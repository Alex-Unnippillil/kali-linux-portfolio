# Copy this file to .env.local and replace placeholder values with your project secrets.

# Required configuration
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your-recaptcha-site-key
NEXT_PUBLIC_SERVICE_ID=your-emailjs-service-id
NEXT_PUBLIC_TEMPLATE_ID=your-emailjs-template-id
NEXT_PUBLIC_USER_ID=your-emailjs-public-key
RECAPTCHA_SECRET=your-recaptcha-secret

# Optional analytics and feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_TRACKING_ID=G-XXXXXXXXXX
NEXT_PUBLIC_VERCEL_ENV=
NEXT_PUBLIC_DEMO_MODE=false
NEXT_PUBLIC_UI_EXPERIMENTS=false
NEXT_PUBLIC_STATIC_EXPORT=false
NEXT_PUBLIC_SHOW_BETA=
FEATURE_TOOL_APIS=disabled
FEATURE_HYDRA=disabled

# Admin API access
ADMIN_READ_KEY=admin-read-key

# Third-party integrations
NEXT_PUBLIC_YOUTUBE_API_KEY=your-youtube-api-key
NEXT_PUBLIC_CURRENCY_API_URL=
NEXT_PUBLIC_BEEF_URL=
NEXT_PUBLIC_GHIDRA_URL=
NEXT_PUBLIC_GHIDRA_WASM=

# Supabase (optional)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=public-anon-key
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=service-anon-key
SUPABASE_SERVICE_ROLE_KEY=service-role-key
