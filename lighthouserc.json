{
  "ci": {
    "collect": {
      "staticDistDir": "./out",
      "numberOfRuns": 1,
      "settings": {
        "budgetsPath": "lighthouse-budget.json"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.9 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
