{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "pages/api/**/*.{js,ts}": { "runtime": "@vercel/node@5.3.20" },
    "app/api/**/route.{js,ts}": { "runtime": "@vercel/node@5.3.20" }
  },
  "env": {
    "ADMIN_READ_KEY": "@portfolio-admin-read-key",
    "RECAPTCHA_SECRET": "@portfolio-recaptcha-secret",
    "SUPABASE_URL": "@portfolio-supabase-url",
    "SUPABASE_SERVICE_ROLE_KEY": "@portfolio-supabase-service-role-key",
    "SUPABASE_ANON_KEY": "@portfolio-supabase-anon-key",
    "NEXT_PUBLIC_SUPABASE_URL": "@portfolio-supabase-url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@portfolio-supabase-anon-key",
    "NEXT_PUBLIC_RECAPTCHA_SITE_KEY": "@portfolio-recaptcha-site-key",
    "NEXT_PUBLIC_SERVICE_ID": "@portfolio-email-service-id",
    "NEXT_PUBLIC_TEMPLATE_ID": "@portfolio-email-template-id",
    "NEXT_PUBLIC_USER_ID": "@portfolio-email-user-id",
    "NEXT_PUBLIC_TRACKING_ID": "@portfolio-ga-tracking-id",
    "NEXT_PUBLIC_YOUTUBE_API_KEY": "@portfolio-youtube-api-key"
  }
}
