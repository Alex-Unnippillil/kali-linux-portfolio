{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "corepack enable && yarn install --immutable",
  "buildCommand": "yarn build",
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  },
  "functions": {
    "pages/api/**/*.{js,ts}": {
      "runtime": "@vercel/node@5.3.20"
    },
    "app/api/**/route.{js,ts}": {
      "runtime": "@vercel/node@5.3.20"
    }
  }
}
